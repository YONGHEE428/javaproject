package day0218;

import java.util.Iterator;

public class VariableTest_01 {

	public static void main(String[] args) {
		
		int a=100;
		int b = 200;
		int c = a + b;
		
		System.out.println(a + "+" + b + "=" + c);
		
		String d = "10";
		String e = "20";
		String f = d + e;
		
		System.out.println(d + " + " + e + " = " + f);
		
	}

}
