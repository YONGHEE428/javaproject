package day0219;

import java.util.Scanner;

public class IfMax_10 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int x, y, z,max;
		
		System.out.println("3개의 수를 입력하시오.");
		x = sc.nextInt();
		y = sc.nextInt();
		z = sc.nextInt();
		
		// If문
		
		if (x > y && y > z) {
			max = x;
			System.out.println("max = " + max);
		} else if (z> x && x >y) {
			max = y;
			System.out.println("max = " + max);
		} else
			max = z;
			System.out.println("max = " + max);

			
			
		
	}
}
